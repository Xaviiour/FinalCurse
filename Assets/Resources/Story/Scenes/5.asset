  sentences:
  - text: "Elric\u2026 I wanted to tell you something that\u2019s been on my mind
      for a while."
    speaker: {fileID: 11400000, guid: bb4cbe5db100b48499ae6e87f55c5b63, type: 2}
    actions:
    - speaker: {fileID: 11400000, guid: bb4cbe5db100b48499ae6e87f55c5b63, type: 2}
